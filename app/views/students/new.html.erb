<h1>Create A New Account!</h1>

<%= form_for @student do |f| %>
  <%= f.label("email") %>
  <%= f.text_field(:email) %>
  <%= f.label("name") %>
  <%= f.text_field(:name) %>
  <%= f.label("password") %>
  <%= f.password_field(:password) %>
  <%= f.label("gender") %>
  <%= f.radio_button(:gender, "male") %>
  <%= f.label("male") %>
  <%= f.radio_button(:gender, "female") %>
  <%= f.label("female") %>
  <%= f.submit("log into scholar") %>
<% end %>
